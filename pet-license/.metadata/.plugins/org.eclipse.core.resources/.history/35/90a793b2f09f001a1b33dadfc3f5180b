			 <script type="text/javascript">
            $(".myselect").select2();
            $(document).ready(function () {
            	if($('#noOfFamilyMembers').val()<=0)
            		{
            		debugger;
                	document.getElementById('buttonss').style.display='none';

                	document.getElementById('buttoncc').style.display='block';
            		}
            	else
            		{document.getElementById('buttonss').style.display='block';

                	document.getElementById('buttoncc').style.display='none';}
            	
            	
            	
            	   	$.ajax({
            	    url: "get-district",
            	    type: 'GET',
            	    datatype:"json",
            	    contentType: "application/json; charset=utf-8",
            	    success: function (responce)
            	    {
            	    	var htm = "<option value=''> Select District</option>";
            	    	 var data = JSON.stringify(responce);
            	         	var obj = JSON.parse(data);
            	             $.each(obj, function(index,item) {        
            	htm = htm +' <option value='+item.districtCode+'>'+item.districtName+'</option>';
            	             });
            	             htm = htm;
            	             $('#tfdistrict').html(htm);  
            	             }      
            	});
});
          
                     
            function loadUlb()
            {
            	  var district= $('#tfdistrict').val();
                $.ajax({
            	    url: "get-ulb",
            	    type: 'GET',
            	    data:{"district":district},
            	    datatype:"json",
            	    contentType: "application/json; charset=utf-8",
            	    success: function (responce)
            	    {
            	    	var htm = "<option value=''> Select Town Name</option>";
            	    	 var data = JSON.stringify(responce);
            	         	var obj = JSON.parse(data);
            	             $.each(obj, function(index,item) {        
            	htm = htm +' <option value='+item.ulbId+'>'+item.ulbName+'</option>';
            	             });
            	             htm = htm;
            	             $('#tfulb').html(htm);  
            	             }      
            	});
       	
            	
            }
            
            
            function loadLocality()
            {
            	  var ulb= $('#tfulb').val();
                $.ajax({
            	    url: "get-locality",
            	    type: 'GET',
            	    data:{"ulb":ulb},
            	    datatype:"json",
            	    contentType: "application/json; charset=utf-8",
            	    success: function (responce)
            	    {
            	    	debugger;
            	    	var htm = "<option value=''> Select Locality</option>";
            	    	 var data = JSON.stringify(responce);
            	         	var obj = JSON.parse(data);
            	             $.each(obj, function(index,item) {        
            	htm = htm +' <option value='+item.localityCode+'>'+item.localityName+'</option>';
            	             });
            	             htm = htm;
            	             $('#tflocality').html(htm);  
            	             }      
            	});
            }
            
            function loadData()
            {
            	document.getElementById('dataform').style.display='block';
            	document.getElementById('buttonForm').style.display='none';
}

            function loadForm()
            
            {	window.location="/covid-new/dashboard";
            	
            
            }
            	
           
        </script>
